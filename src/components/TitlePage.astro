---
import Title from './Title.astro'
import Shape from './icons/Shape.astro'
type Props = {
	title: string
}

const { title } = Astro.props
---

<div class='flex justify-start items-center gap-4 title'>
	<a href="/" aria-label="link to go home page" aria-describedby="home"><Shape /></a>
	<Title>{title}</Title>
</div>
